{
  "name": "gulp-group-concat",
  "version": "1.1.5",
  "description": "Concats groups of files into a smaller number of files",
  "main": "index.js",
  "repository": "http://github.com/TakenPilot/gulp-group-concat.git",
  "scripts": {
    "test": "./node_modules/.bin/mocha -r blanket ",
    "coveralls": "./node_modules/.bin/mocha -r blanket -R mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "config": {
    "blanket": {
      "pattern": "index.js",
      "data-cover-never": "node_modules"
    }
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "gulpfriendly",
    "concat",
    "group",
    "filter",
    "sourcemaps",
    "multi",
    "pipe",
    "stream"
  ],
  "author": "Dane Stuckel",
  "license": "ISC",
  "dependencies": {
    "concat-with-sourcemaps": "^1.0.0",
    "globule": "^1.2.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-util": "^3.0.3",
    "lodash": "^4.17.4",
    "through2": "^2.0.3",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^4.1.0",
    "coveralls": "^2.11.2",
    "gulp": "^3.8.11",
    "gulp-pleeease": "^2.0.2",
    "gulp-sourcemaps": "^1.3.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "sinon": "^2.4.1",
    "vinyl-buffer": "^1.0.0"
  }
}
